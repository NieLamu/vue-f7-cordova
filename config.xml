<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.NieLamu" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>VUE F7 CORDOVA</name>
    <description>
        A sample Framework7-Vue application Cordova application that responds to the deviceready event.
    </description>
    <author email="mxmxlty@gmail.com" href="NieLamu.github.io">
        NieLamu
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
